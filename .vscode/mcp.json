{
  "servers": {
    "powershell-integration": {
      "command": "pwsh.exe",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        "${env:MCP_DIR}/mcp-powershell-exec/start-server.ps1"
      ],
      "env": {}
    },
    "powershell-integration-uv": {
      "command": "uv",
      "args": ["run", "${env:MCP_DIR}/mcp-powershell-exec/server.py"],
      "env": {}
    }
  }
}
